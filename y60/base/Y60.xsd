Example:
&lt;sampler3d name='bar'&gt;1&lt;/sampler3d&gt;</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:int">
                    <xs:attribute name="name" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Name of the property</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
